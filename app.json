{
  "expo": {
    "name": "PlowIt",
    "slug": "plowit",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "splash": {
      "image": "./assets/appSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A snow plow job management and hazard reporting application\n\nBuilt during HackBU 2020 in Bingamton, NY",
    "githubUrl": "https://github.com/milorue/plowme"
  }
}
